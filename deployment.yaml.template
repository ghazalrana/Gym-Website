apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    run: app
  name: app
spec:
  replicas: 2
  selector:
    matchLabels:
      run: app
  template:
    metadata:
      labels:
        run: app
    spec:
      containers:
      - image: gcr.io/websocket-285908/gym-circleci:SHORT_HASH
        name: app
        ports:
        - containerPort: 80
